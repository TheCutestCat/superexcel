{
    "version": 2,
    "builds": [
      {
        "src": "superexcel/main.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "superexcel/main.py"
      }
    ],
    "env": {
      "PYTHONPATH": "/var/task"
    }
  }